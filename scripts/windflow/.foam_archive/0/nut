FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 3.16e-6;

boundaryField
{
	inlet
	{
		type				freestream;
		freestreamValue		$internalField;
	}

	ground
	{
		type			nutUSpaldingWallFunction;
		value			uniform 0;
	}
	
	walls
	{
		type			zeroGradient;
	}
	
	top
	{
		type			zeroGradient;
	}
}


