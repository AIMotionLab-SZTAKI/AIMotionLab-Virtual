FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

#includeEtc "caseDicts/setConstraintTypes"

boundaryField
{
	inlet
	{
		type	zeroGradient;
	}

	ground
	{
		type	zeroGradient;
	}
	
	walls
	{
		type	fixedValue;
		value	uniform 0;
	}

	top
	{
		type	fixedValue;
		value	uniform 0;
	}
}


