FoamFile
{
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  6;

method              scotch;

simpleCoeffs
{
    n               (1 6 1); // total must match numberOfSubdomains
}

hierarchicalCoeffs
{
    n               (1 6 1); // total must match numberOfSubdomains
    order           xyz;
}


